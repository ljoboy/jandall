<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity ExtraInfo.
    -->
    <changeSet id="20200324234200-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="social_id"
                                 baseTableName="extra_info"
                                 constraintName="fk_extra_info_social_id"
                                 referencedColumnNames="id"
                                 referencedTableName="social"/>

        <addForeignKeyConstraint baseColumnNames="privacy_id"
                                 baseTableName="extra_info"
                                 constraintName="fk_extra_info_privacy_id"
                                 referencedColumnNames="id"
                                 referencedTableName="privacy"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="extra_info"
                                 constraintName="fk_extra_info_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
